RewriteEngine On
RewriteBase /school/

RewriteRule ^staff/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)$ staff/$1/index.php?stype=$2&uid=$3
RewriteRule ^staff/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/$ staff/$1/index.php?stype=$2&uid=$3

RewriteRule ^students/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)$ students/$1/index.php?uid=$2
RewriteRule ^students/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/$ students/$1/index.php?uid=$2

RewriteRule ^([a-zA-Z0-9]+)$ $1.php
RewriteRule ^([a-zA-Z0-9]+)/$ $1.php
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)$ $1/$2.php
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/$ $1/$2.php
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)$ $1/$2/$3.php
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/$ $1/$2/$3.php
#use [NC, L] if you want to strictly use the beautified urls